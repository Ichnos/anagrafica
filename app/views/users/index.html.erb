<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Username</th>
      <th>Email</th>
      <th>Birth city</th>
      <th>Birth date</th>
      <th>Whatsapp</th>
      <th>Facebook</th>
      <th>Skype</th>
      <th>Google plus</th>
      <th>Twitter</th>
      <th>Mov five star</th>
      <th>Meet up</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.surname %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.birth_city %></td>
        <td><%= user.birth_date %></td>
        <td><%= user.whatsApp %></td>
        <td><%= user.facebook %></td>
        <td><%= user.skype %></td>
        <td><%= user.google_plus %></td>
        <td><%= user.twitter %></td>
        <td><%= user.mov_five_star %></td>
        <td><%= user.meet_up %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
